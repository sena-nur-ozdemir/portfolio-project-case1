@model List<Case1ResumeProject.Entities.Category>

<!-- portfolio filter (desktop) -->
<ul class="portfolio-filter list-inline wow fadeInUp">
    <li class="current list-inline-item" data-filter="*">Tümü</li>
    @foreach (var item in Model)
    {
        <li class="list-inline-item" data-filter=".@item.CategorySlug">@item.CategoryName</li>
    }
</ul>